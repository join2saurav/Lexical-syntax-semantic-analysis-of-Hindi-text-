
import re
import sys
reload(sys)
sys.setdefaultencoding('utf-8')

g = open("depth_13.txt","w")
with open("depth_10.txt","r") as f:
    lines=f.read().splitlines()
for line in lines:
    line1=line.replace("_NNPC","")
    line2=line1.replace("_VJJ","")
    line3 = line2.replace("_NNP","")
    line4 = line3.replace("_RB","")
    line5 = line4.replace("_JVB","")
    line6 = line5.replace("_VAUX","")
    line7 = line6.replace("_DT","")
    line8 = line7.replace("_CC","")
    line9 = line8.replace("_PUNC","")
    line10 = line9.replace("_VFM", "")
    line11 = line10.replace("_JJ", "")
    line12 = line11.replace("_NVB", "")
    line13 = line12.replace("_VB", "")
    line14 = line13.replace("_NLOC", "")
    line15 = line14.replace("_RP", "")
    line16 = line15.replace("_INTF", "")
    line17 = line16.replace("_NN", "")
    line18 = line17.replace("_PRP", "")
    line19 = line18.replace("_PREP", "")
    line20 = line19.replace("_QFNUM", "")
    line21 = line20.replace("_QF", "")
    line22 = line21.replace("_VNN", "")
    line23 = line22.replace("_NEG", "")
    line24 = line23.replace("C", "")
    line25 = line24.replace("'_SYM", "")
    line26 = line25.replace("_SYM", "")
    line27 = line26.replace("_QW", "")
    line28 = line27.replace("_VRB", "")
    line29 = line28.replace("_-None-", "")
    line30 = line29.replace("_", "")
    line31 = line30.replace("'", "")
    line32 = line31.replace("- one-", "")
    g.write(line32)
    g.write("\n")
